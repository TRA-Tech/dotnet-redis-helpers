<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>TraTech.RedisHelpers</Title>
    <Version>0.0.1</Version>
    <Authors>TraTech</Authors>
    <Company>TraTech</Company>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>Redis services and middlewares for .NET</Description>
    <RepositoryUrl>https://github.com/TRA-Tech/dotnet-redis-helpers</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="7.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="StackExchange.Redis" Version="2.6.111" />
  </ItemGroup>

</Project>
